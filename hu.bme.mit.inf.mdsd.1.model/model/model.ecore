<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="model" nsURI="http://model/1.0" nsPrefix="model">
  <eClassifiers xsi:type="ecore:EClass" name="TeamData">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timeoutTimer" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="_timeoutState" eType="#//State"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="foulCounter" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EShort"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Player" eSuperTypes="#//TeamMember">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="position" eType="#//Position"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="shirtNo" lowerBound="1"
        upperBound="99" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EShort"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="goals" upperBound="-1"
        eType="#//Event"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="yellowCard" eType="#//Event"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="_hasYellow" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Type">
    <eLiterals name="League"/>
    <eLiterals name="Cup" value="1"/>
    <eLiterals name="SuperCup" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="AgeGroup">
    <eLiterals name="Adult"/>
    <eLiterals name="U19" value="1"/>
    <eLiterals name="U17" value="2"/>
    <eLiterals name="U15" value="3"/>
    <eLiterals name="U13" value="4" literal="U13"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Role">
    <eLiterals name="HeadCoach"/>
    <eLiterals name="TechnicalCoach" value="5"/>
    <eLiterals name="GoalkeeperCoach" value="6"/>
    <eLiterals name="Coach" value="1"/>
    <eLiterals name="Medical" value="2"/>
    <eLiterals name="Masseur" value="3"/>
    <eLiterals name="Other" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Position">
    <eLiterals name="Goalkeeper"/>
    <eLiterals name="FieldPlayer" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Match">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="date" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="location" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//Type"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ageGroup" eType="#//AgeGroup"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Referee" lowerBound="1"
        eType="#//Person" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Assistant" lowerBound="1"
        eType="#//Person" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ThirdReferee" lowerBound="1"
        eType="#//Person" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Supervisor" lowerBound="1"
        eType="#//Person" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="teams" lowerBound="2" upperBound="2"
        eType="#//Team" containment="true" eOpposite="#//Team/match"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="events" upperBound="-1"
        eType="#//Event" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="clock" lowerBound="1" eType="#//Clock"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Person">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Team">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="firstHalfData" lowerBound="1"
        eType="#//TeamData" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="counters" upperBound="-1"
        eType="#//RedCounter" eOpposite="#//RedCounter/team"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="captain" lowerBound="1"
        eType="#//Player" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="startingLine" lowerBound="5"
        upperBound="5" eType="#//Player" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="substitutes" lowerBound="3"
        upperBound="7" eType="#//Player" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="staff" upperBound="-1"
        eType="#//StaffMember" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="match" lowerBound="1" eType="#//Match"
        eOpposite="#//Match/teams"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="notFirstHalfData" lowerBound="1"
        eType="#//TeamData" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="goals" upperBound="-1"
        eType="#//Event"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="notSentOffMembers" upperBound="-1"
        eType="#//TeamMember"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="_hasTimeout" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="penalties" upperBound="-1"
        eType="#//Penalty.1" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RedCounter">
    <eStructuralFeatures xsi:type="ecore:EReference" name="team" lowerBound="1" eType="#//Team"
        eOpposite="#//Team/counters"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="_state" eType="#//State"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="_timeLeft" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TeamMember" abstract="true" eSuperTypes="#//Person">
    <eStructuralFeatures xsi:type="ecore:EReference" name="team" lowerBound="1" eType="#//Team"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="events" upperBound="-1"
        eType="#//Event" eOpposite="#//Event/teamMember"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="redCard" eType="#//Event"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="_hasRed" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StaffMember" eSuperTypes="#//TeamMember">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="role" eType="#//Role"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Event">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="teamMember" lowerBound="1"
        eType="#//TeamMember" eOpposite="#//TeamMember/events"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//EventType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="redCounter" eType="#//RedCounter"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StaffPlayer" eSuperTypes="#//StaffMember #//Player"/>
  <eClassifiers xsi:type="ecore:EEnum" name="EventType">
    <eLiterals name="Goal"/>
    <eLiterals name="RedCard" value="1"/>
    <eLiterals name="YellowCard" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Clock">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timer" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="counters" eType="#//RedCounter"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="halfTimeLong" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="extraHalfTimeLong" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="_timePart" eType="#//TimePart"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="state" eType="#//State"
        derived="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="TimePart">
    <eLiterals name="FirstHalf"/>
    <eLiterals name="SecondHalf" value="1" literal="SecondHalf"/>
    <eLiterals name="ExtraFirstHalf" value="2"/>
    <eLiterals name="ExtraSecondHalf" value="3"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="State">
    <eLiterals name="Active"/>
    <eLiterals name="NotActive" value="1"/>
    <eLiterals name="Waiting" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="PlayerType">
    <eLiterals name="Start"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Penalty">
    <eLiterals name="Success"/>
    <eLiterals name="Failed" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Penalty">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isSuccess" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
</ecore:EPackage>
