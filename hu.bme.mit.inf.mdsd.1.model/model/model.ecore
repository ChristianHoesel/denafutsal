<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="model" nsURI="http://model/1.0" nsPrefix="model">
  <eClassifiers xsi:type="ecore:EClass" name="Game">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="report" lowerBound="1"
        eType="#//Report" eOpposite="#//Report/game"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="board" lowerBound="1" eType="#//Board"
        eOpposite="#//Board/game"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Report">
    <eStructuralFeatures xsi:type="ecore:EReference" name="teams" lowerBound="2" upperBound="2"
        eType="#//Team"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="date" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="location" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="supervisor" lowerBound="1"
        eType="#//Supervisor"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="thirdReferee" lowerBound="1"
        eType="#//ThirdReferee"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="assistant" lowerBound="1"
        eType="#//Assistant"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="referee" lowerBound="1"
        eType="#//Referee"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="round" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EShort"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//Type"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="ageGroup" eType="#//AgeGroup"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="game" lowerBound="1" eType="#//Game"
        eOpposite="#//Game/report"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Board">
    <eStructuralFeatures xsi:type="ecore:EReference" name="redCounters" upperBound="-1"
        eType="#//RedCounter"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="teamData" lowerBound="2"
        upperBound="2" eType="#//TeamData"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="game" lowerBound="1" eType="#//Game"
        eOpposite="#//Game/board"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Team">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="captain" lowerBound="1"
        eType="#//Player"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="startingLine" lowerBound="5"
        upperBound="5" eType="#//Player"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="substitues" lowerBound="3"
        upperBound="7" eType="#//Player"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="staff" upperBound="-1"
        eType="#//Staff"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="TeamData">
    <eStructuralFeatures xsi:type="ecore:EReference" name="team" lowerBound="1" eType="#//Team"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="foulCounter" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EShort"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="timeoutTimer" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="redCounters" upperBound="-1"
        eType="#//RedCounter"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RedCounter">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Person">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Assistant" eSuperTypes="#//Person"/>
  <eClassifiers xsi:type="ecore:EClass" name="Referee" eSuperTypes="#//Person"/>
  <eClassifiers xsi:type="ecore:EClass" name="ThirdReferee" eSuperTypes="#//Person"/>
  <eClassifiers xsi:type="ecore:EClass" name="Supervisor" eSuperTypes="#//Person"/>
  <eClassifiers xsi:type="ecore:EClass" name="TeamMember" eSuperTypes="#//Person">
    <eStructuralFeatures xsi:type="ecore:EReference" name="redCard" eType="#//RedCard"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="yellowCards" upperBound="2"
        eType="#//YellowCard"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Player" eSuperTypes="#//TeamMember">
    <eStructuralFeatures xsi:type="ecore:EReference" name="scores" upperBound="-1"
        eType="#//Goal"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="shirt" lowerBound="1" upperBound="99"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="position" eType="#//Position"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="team" lowerBound="1" eType="#//Team"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Staff" eSuperTypes="#//TeamMember">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="role" eType="#//Role"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Goal">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Card">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="time" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//ELong"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="RedCard" eSuperTypes="#//Card"/>
  <eClassifiers xsi:type="ecore:EClass" name="YellowCard" eSuperTypes="#//Card"/>
  <eClassifiers xsi:type="ecore:EEnum" name="Type">
    <eLiterals name="League"/>
    <eLiterals name="Cup" value="1"/>
    <eLiterals name="SuperCup" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="AgeGroup">
    <eLiterals name="Adult"/>
    <eLiterals name="U19" value="1"/>
    <eLiterals name="U17" value="2"/>
    <eLiterals name="U15" value="3"/>
    <eLiterals name="U13" value="4" literal="U13"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Role">
    <eLiterals name="HeadCoach"/>
    <eLiterals name="TechnicalCoach" value="5"/>
    <eLiterals name="GoalkeeperCoach" value="6"/>
    <eLiterals name="Coach" value="1"/>
    <eLiterals name="Medical" value="2"/>
    <eLiterals name="Masseur" value="3"/>
    <eLiterals name="Other" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Position">
    <eLiterals name="Goalkeeper"/>
    <eLiterals name="FieldPlayer" value="1"/>
  </eClassifiers>
</ecore:EPackage>
